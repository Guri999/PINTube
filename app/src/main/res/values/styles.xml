<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.Theme.ColorContacts.ButtonBar.Fullscreen" parent="">
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
        <item name="android:fontFamily">@font/roboto_medium</item>
    </style>

    <style name="customTextViewFontStyle" parent="@android:style/Widget.DeviceDefault.TextView">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="customEditTextFontStyle" parent="@android:style/Widget.DeviceDefault.EditText">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="customAppBarOverlay" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="contentInsetStart">0dp</item>
        <item name="contentInsetStartWithNavigation">0dp</item>
    </style>

    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/ic_splash_pintube</item>
    </style>
</resources>